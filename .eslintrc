{
  "parserOptions": {
    "ecmaVersion": 2020,
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true
    }
  },
  "env": {
    "node": true,
    "es6": true,
    "browser": true
  },
  "extends": [
    "airbnb"
  ],
  "plugins": [
    "import",
    "react",
    "react-hooks"
  ],
  "rules": {
    "comma-dangle": ["error", "never"],
    "semi": [2, "never"],
    "react/jsx-filename-extension": [1, {"extensions": [".js", ".jsx"]}],
    "no-unused-vars": 1
  }
}
